<package>
  <name>cob_texture_categorization</name>
  <version>0.1.0</version>
  <description>

     Image processing tool for the categorization of textures.

  </description>
  <author email="richard.bormann@ipa.fraunhofer.de">Richard Bormann, Daniel Hundsd√∂rfer</author>
  <maintainer email="jsf@ipa.fhg.de">Jan Fischer</maintainer>
  <license>LGPL</license>
  <url>http://ros.org/wiki/cob_texture_categorization</url>

  <buildtool_depend>catkin</buildtool_depend>

  <build_depend>libopencv-dev</build_depend>
  <build_depend>pcl_ros</build_depend>
  <build_depend>cmake_modules</build_depend> <!-- needed for tinyxml, boost opencv, or eigen -->
  <build_depend>roscpp</build_depend>
  <build_depend>roslib</build_depend>
  <build_depend>cv_bridge</build_depend>
  <build_depend>sensor_msgs</build_depend>
  <build_depend>message_filters</build_depend>
  <build_depend>image_transport</build_depend>
  <build_depend>libpcl-all-dev</build_depend>
  <build_depend>libvlfeat</build_depend>
  <build_depend>cob_surface_classification</build_depend>

  <run_depend>libopencv-dev</run_depend>
  <run_depend>pcl_ros</run_depend>
  <run_depend>roscpp</run_depend>
  <run_depend>roslib</run_depend>
  <run_depend>cv_bridge</run_depend>
  <run_depend>sensor_msgs</run_depend>
  <run_depend>message_filters</run_depend>
  <run_depend>image_transport</run_depend>
  <run_depend>libpcl-all</run_depend>
  <run_depend>libvlfeat</run_depend>
